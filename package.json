{
  "name": "hc-pdf-server",
  "version": "1.0.0",
  "description": "html to pdf rendering server using Headless Chrome",
  "main": "dist/server.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "tsc",
    "test": "tap test/**/*.test.ts",
    "start": "node dist/server.js",
    "dev": "ts-node ./src/server.ts",
    "lint": "eslint --fix ./ --ext ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/uyamazak/hc-pdf-server.git"
  },
  "keywords": ["pdf", "headless chrome", "puppeteer"],
  "author": "uyamazak <yu.yamazaki85@gmail.com> (https://github.com/uyamazak/)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/uyamazak/hc-pdf-server/issues"
  },
  "homepage": "https://github.com/uyamazak/hc-pdf-server#readme",
  "dependencies": {
    "fastify": "^3.8.0",
    "fastify-autoload": "^3.3.1",
    "fastify-bearer-auth": "^5.0.2",
    "fastify-cli": "^2.5.1",
    "fastify-formbody": "^5.0.0",
    "fastify-plugin": "^3.0.0",
    "puppeteer": "~5.4.1"
  },
  "devDependencies": {
    "@types/node": "^14.14.7",
    "@types/puppeteer": "^5.4",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.7.0",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^2.1.2",
    "tap": "^14.0.0",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.5"
  }
}
